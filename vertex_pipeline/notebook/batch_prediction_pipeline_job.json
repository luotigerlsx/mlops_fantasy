              "{{$.inputs.parameters['project_id']}}",
              "--data-region-output-path",
              "{{$.inputs.parameters['data_region']}}",
              "--gcs-output-folder-output-path",
              "{{$.inputs.parameters['gcs_output_folder']}}",
              "--gcs-output-format-output-path",
